{
    "variables": {
        "path_screenshot_steam": "",
        "path_screenshot_custom": "",
        "path_game": "",
        "path_ttc": "",
        "ttc_realm": "eu",
        "steam_game_id": "306130"
    },
    "ttc": {
        "data": {
            "age_test_file": "{path_ttc}\\PriceTable.lua",
            "extraction_path": "{path_ttc}",
            "max_age_minutes": 30
        },
        "download": {
            "zip_file_name": "TamrielTradeCentrePriceDataTemp.zip",
            "max_age_minutes": 20,
            "source_url": "https://{ttc_realm}.tamrieltradecentre.com/download/PriceTable"
        }
    },
    "game": {
        "path": "{path_game}",
        "exe": "eso64.exe"
    },
    "screenshot": {
        "original": {
            "path": "{path_screenshot_custom}",
            "name": "eso64 %Y-%m-%d %H-%M-%S.png"
        },
        "converted": [
            {
                "path": "{path_screenshot_steam}\\remote\\\\screenshots",
                "name": "%Y%m%d%H%M%S_1.jpg",
                "resize": null,
                "quality": 96
            },
            {
                "path": "{path_screenshot_steam}\\remote\\{steam_game_id}\\screenshots\\thumbnails",
                "name": "%Y%m%d%H%M%S_1.jpg",
                "resize": [200,112],
                "quality": 50
            }
        ],
        "metadata": {
            "path": "{path_screenshot_steam}\\screenshots.vdf",
            "regex_in": [
                []
            ],
            "regex_out": [
                []
            ],
            "filename": ""
        }
    },
    "cleanup": {
        "folders": "__pycache__"
    }
}